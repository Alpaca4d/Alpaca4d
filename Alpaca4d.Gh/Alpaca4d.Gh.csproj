  <!-- ==========================================
       BUILD TARGETS
       ========================================== -->
  <Target Name="CopyToGrasshopperLibraries" AfterTargets="Build">
    <MakeDir Directories="$(GrasshopperLibrariesPath)/Alpaca4d/" />
    <ItemGroup>
      <Dlls Include="$(OutDir)*.dll" />
      <Ghas Include="$(OutDir)Alpaca4d.Gh.gha" />
    </ItemGroup>
    <Copy SourceFiles="@(Dlls)" DestinationFolder="$(GrasshopperLibrariesPath)/Alpaca4d/" />
    <Copy SourceFiles="@(Ghas)" DestinationFolder="$(GrasshopperLibrariesPath)/Alpaca4d/" />
    
    <!-- Copy OpenSees-Solvers folder -->
    <ItemGroup>
      <OpenSeesFiles Include="$(OutDir)OpenSees-Solvers\**\*" />
    </ItemGroup>
    <Copy SourceFiles="@(OpenSeesFiles)" DestinationFolder="$(GrasshopperLibrariesPath)/Alpaca4d/OpenSees-Solvers\%(RecursiveDir)" />
    
    <!-- Copy data.bin file -->
    <Copy SourceFiles="$(OutDir)data.bin" DestinationFolder="$(GrasshopperLibrariesPath)/Alpaca4d/" />
    
    <!-- Copy UserObject folder -->
    <ItemGroup>
      <UserObjectFiles Include="$(OutDir)UserObject\**\*" />
    </ItemGroup>
    <Copy SourceFiles="@(UserObjectFiles)" DestinationFolder="$(GrasshopperLibrariesPath)/Alpaca4d/UserObject\%(RecursiveDir)" />
  </Target>

