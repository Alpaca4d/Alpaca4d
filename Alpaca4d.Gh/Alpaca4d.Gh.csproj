<Project Sdk="Microsoft.NET.Sdk">
	
  <!-- ==========================================
       PROJECT PROPERTIES
       ========================================== -->
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <Version>1.0</Version>
    <Title>Alpaca4d</Title>
    <Description>Description of Alpaca4d</Description>
    <TargetExt>.gha</TargetExt>
    <BaseOutputPath></BaseOutputPath>
    <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
  </PropertyGroup>

  <!-- ==========================================
       DEBUG CONFIGURATION
       ========================================== -->
  <!-- Debug configuration for Windows - Rhinoceros 8 (Primary) -->
  <PropertyGroup Condition="$(Configuration) == 'Debug' AND $([MSBuild]::IsOSPlatform(Windows))">
    <StartProgram>C:\Program Files\Rhino 8\System\Rhino.exe</StartProgram>
    <StartArguments></StartArguments>
    <StartAction>Program</StartAction>
  </PropertyGroup>

  <!-- Debug configuration for Windows - Rhinoceros 7 (Fallback) -->
  <PropertyGroup Condition="$(Configuration) == 'Debug' AND $([MSBuild]::IsOSPlatform(Windows)) AND !Exists('C:\Program Files\Rhino 8\System\Rhino.exe')">
    <StartProgram>C:\Program Files\Rhino 7\System\Rhino.exe</StartProgram>
    <StartArguments></StartArguments>
    <StartAction>Program</StartAction>
  </PropertyGroup>

  <!-- Debug configuration for macOS - Rhinoceros 8 (Primary) -->
  <PropertyGroup Condition="$(Configuration) == 'Debug' AND $([MSBuild]::IsOSPlatform(OSX))">
    <StartProgram>/Applications/Rhino 8.app/Contents/MacOS/Rhinoceros</StartProgram>
    <StartArguments></StartArguments>
    <StartAction>Program</StartAction>
  </PropertyGroup>

  <!-- Debug configuration for macOS - Rhinoceros 7 (Fallback) -->
  <PropertyGroup Condition="$(Configuration) == 'Debug' AND $([MSBuild]::IsOSPlatform(OSX)) AND !Exists('/Applications/Rhino 8.app/Contents/MacOS/Rhinoceros')">
    <StartProgram>/Applications/Rhino 7.app/Contents/MacOS/Rhinoceros</StartProgram>
    <StartArguments></StartArguments>
    <StartAction>Program</StartAction>
  </PropertyGroup>

  <!-- ==========================================
       GRASSHOPPER LIBRARIES PATH
       ========================================== -->
  <PropertyGroup>
    <GrasshopperLibrariesPath Condition="'$(OS)' == 'Windows_NT'">$(APPDATA)\Grasshopper\Libraries</GrasshopperLibrariesPath>
    <GrasshopperLibrariesPath Condition="'$(OS)' != 'Windows_NT'">$(HOME)/Library/Application Support/McNeel/Rhinoceros/MacPlugIns/Grasshopper/Libraries</GrasshopperLibrariesPath>
  </PropertyGroup>

  <!-- ==========================================
       PACKAGE REFERENCES
       ========================================== -->
  <ItemGroup>
    <PackageReference Include="Grasshopper" Version="7.18.22124.3001" IncludeAssets="compile;build" />
    <PackageReference Include="RhinoCommon" Version="7.18.22124.3001" />
    <PackageReference Include="System.Resources.Extensions" Version="7.0.0" />
  </ItemGroup>

  <!-- ==========================================
       PROJECT REFERENCES
       ========================================== -->
  <ItemGroup>
    <ProjectReference Include="..\Alpaca.Core\Alpaca4d.Core.csproj" />
  </ItemGroup>

  <!-- ==========================================
       REFERENCES
       ========================================== -->
  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>

  <!-- ==========================================
       RESOURCE FILES
       ========================================== -->
  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  
  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

